// Arreglos

// Los arreglos sirven para agrupar elementos del mismo tipo

const numbers = [10, 20, 30]; //Los arreglos se abren con CORCHETES [], a diferencia de los objetos 

// Otra forma
const months = new Array('Enero', 'Febrero', 'Marzo');

console.log(numbers);
console.log(months);

// Un arreglo que contiene datos de todo tipo

const all = ['Hola', 10, true, 's√≠', null, {name: 'Julian', work: 'programador'}, [1, 2, 3]];

console.log(all);
